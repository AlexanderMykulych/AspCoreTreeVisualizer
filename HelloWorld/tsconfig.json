{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    //"sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015",
      "es5",
      "es6"
    ],
    "module": "system",
    "outFile": "wwwroot/dist/app-bandle.js",
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "inlineSources": true
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "ClientApp/css/*.css"
  ],
  "compileOnSave": true
}
